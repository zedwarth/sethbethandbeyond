{
  "name": "sethbethandbeyond.com",
  "version": "0.1.0",
  "description": "sethbethandbeyond.com wedding website",
  "main": "static-src/js/index.js",
  "scripts": {
    "build:css": "node-sass --include-path node_modules/bulma/ src-static/scss/main.scss > static/css/main.css",
    "build": "npm run build:css",
    "watch:css": "node-sass -w --include-path node_modules/bulma/ src-static/scss/main.scss > static/css/main.css",
    "prewatch": "npm run build",
    "watch": "parallelshell 'hugo server --bind 0.0.0.0' 'npm run watch:css'"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git-codecommit.us-east-1.amazonaws.com/v1/repos/sethbethandbeyond.com"
  },
  "keywords": [
    "wedding",
    "hugo",
    "bulma"
  ],
  "author": "Seth Szymanski",
  "license": "MIT",
  "dependencies": {
    "bulma": "^0.5.0",
    "hugo-cli": "^0.4.2",
    "node-sass": "^4.5.3",
    "parallelshell": "^3.0.1"
  }
}

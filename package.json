{
  "name": "sethbethandbeyond.com",
  "version": "0.3.0",
  "description": "sethbethandbeyond.com wedding website",
  "main": "static-src/js/index.js",
  "scripts": {
    "autoprefixer": "postcss -u autoprefixer -r static/css/*",
    "build": "run-p build:css build:webpack",
    "build:css": "npm run autoprefixer",
    "build:hugo": "hugo --disableRSS --disableSitemap",
    "build:webpack": "webpack",
    "clean": "run-p clean:*",
    "clean:static": "rimraf static/css/* static/js/*",
    "clean:public": "rimraf public",
    "postbuild": "npm run build:hugo",
    "prebuild": "npm run clean",
    "prebuild:css": "npm run scss",
    "prewatch:babel": "npm run build:babel",
    "prewatch:css": "npm run build:css",
    "scss": "node-sass --include-path node_modules/bulma/ src-static/scss/main.scss > static/css/main.css",
    "watch": "run-p watch:*",
    "watch:webpack": "onchange 'src-static/js/*.js' -- npm run build:bable",
    "watch:css": "onchange 'src-static/scss/*.scss' -- npm run build:css",
    "watch:hugo": "hugo server --bind 0.0.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zedwarth/sethbethandbeyond.git"
  },
  "keywords": [
    "wedding",
    "hugo",
    "bulma"
  ],
  "author": "Seth Szymanski",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "bulma": "^0.5.0",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "hugo-cli": "^0.4.2",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "onchange": "^3.2.1",
    "postcss-cli": "^4.1.0",
    "postcss-loader": "^2.0.6",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "webpack": "^3.4.1"
  }
}
